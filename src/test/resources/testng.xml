<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <test name="MTP Desktop tests">
        <classes>
            <class name="testng.MtpKotlinTest">
                <methods>
                    <include name=".*Desktop"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="MTP Mobile tests" enabled="false">
        <classes>
            <class name="testng.MtpKotlinTest">
                <methods>
                    <include name=".*Mobile"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
